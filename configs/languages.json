{
	"languages": [
		{
			"name": "C",
			"version": "99",
			"sandbox_files": [
				"main.c"
			],
			"compile_script": [
				"gcc",
				"$sandbox_path/main.c",
				"-o",
				"$sandbox_path/main"
			],
			"run_script": [
				"./main"
			]
		},
		{
			"name": "C++",
			"version": "C++11",
			"sandbox_files": [
				"main.cpp"
			],
			"compile_script": [
				"g++",
				"$file_path",
				"-o",
				"$program_path"
			],
			"run_script": [
				"./main"
			]
		},
		{
			"name": "Java",
			"version": "Java 17",
			"sandbox_files": [
				"HelloWorld.java"
			],
			"compile_script": [
				"javac",
				"$sandbox_path/HelloWorld.java"
			],
			"run_script": [
				"/usr/lib/jvm/java-1.17.0-openjdk-amd64/bin/java",
				"HelloWorld"
			]
		},
		{
			"name": "Go",
			"version": "1.24",
			"sandbox_files": [
				"main.go"
			],
			"compile_script": [
				"/bin/sh",
				"-c",
				"cd $sandbox_path && go mod init main && go mod tidy && go build -o main main.go"
			],
			"run_script": [
				"./main"
			]
		},
		{
			"name": "Python",
			"version": "3.8",
			"sandbox_files": [
				"main.py"
			],
			"run_script": [
				"/usr/bin/python3",
				"main.py"
			]
		}
	]
}
